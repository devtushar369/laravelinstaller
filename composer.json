{
    "name": "hashcode/laravel-instller",
    "description": "A web-based Laravel installer package that simplifies project setup and configuration, enabling developers to start their projects quickly without using the command line.",
    "type": "library",
    "authors": [
        {
            "name": "devtushar369",
            "email": "dev.tusharahmed@gmail.com"
        }
    ],
    "require": {
        "php": "^8.0|^8.3",
        "laravel/framework": "^10.0|^11.0"
    },
    "autoload": {
        "psr-4": {
            "Hashcode\\laravelInstaller\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Hashcode\\laravelInstaller\\Providers\\LaravelInstallerServiceProvider"
            ]
        }
    },

    "minimum-stability": "stable",
    "prefer-stable": true

}
